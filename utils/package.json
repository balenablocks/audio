{
  "name": "audioblock-utils",
  "version": "1.0.0",
  "description": "Utilities for the balena audio block",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf clean",
    "build": "tsc --project tsconfig.json",
    "start": "ts-node src/index.ts",
    "watch": "nodemon --watch src --ignore tests --ext js,ts,json --exec node -r ts-node/register/transpile-only src/index.ts",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/balenablocks/audio.git"
  },
  "keywords": [
    "audio",
    "block",
    "pulseaudio",
    "balena"
  ],
  "author": "Tom√°s Migone <tomas@balena.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/balenablocks/audio/issues"
  },
  "homepage": "https://github.com/balenablocks/audio#readme",
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@unional/fixture": "^1.7.2",
    "ava": "^3.15.0",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "ts-standard": "^10.0.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {},
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
